<link rel="stylesheet" href="/css/seriesEpisodes.css">

<h1>Episodes</h1>

<div id="container">
    {{#each episodes}}
        <div class="episode">
            <a href="/content/viewContent/{{this._id}}"> 
                <img src="/content/thumbnail/{{this.thumbnail}}" width="300px" height="200px"> 
            </a> 
        </div>
    {{/each}}
</div>